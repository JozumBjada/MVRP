Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"getPrototype", "::", "usage"}], "=", 
   "\"\<Basically parses the Function expression corresponding to a given \
action. If phaseshift[#,1,phi]& is supplied to getPrototype, it returns \
{phaseshift,1,None,\[LeftAssociation]'head'\[Rule]phaseshift,'phase'\[Rule]\
phi\[RightAssociation]}\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"getPrototype", "[", "action_Function", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "aux", ",", "head", ",", "path1", ",", "path2", ",", "pars", ",", 
      "proto", ",", "path1pos", ",", "path2pos", ",", "parspos", ",", 
      "parsnames", ",", "parsvalues"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"aux", "=", 
      RowBox[{"Hold", "[", "action", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"head", "=", 
      RowBox[{"aux", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1", ",", "0"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"proto", "=", 
      RowBox[{"prototypes", "[", "head", "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"path1pos", "=", 
      RowBox[{"FirstPosition", "[", 
       RowBox[{"proto", ",", 
        RowBox[{"\"\<path1\>\"", "|", "\"\<path\>\""}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"path1", "=", 
      RowBox[{"aux", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1", ",", 
         RowBox[{"1", "+", 
          RowBox[{"First", "@", "path1pos"}]}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"path2pos", "=", 
      RowBox[{"FirstPosition", "[", 
       RowBox[{"proto", ",", "\"\<path2\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"path2", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"MissingQ", "[", "path2pos", "]"}], ",", 
        "\[IndentingNewLine]", "None", ",", "\[IndentingNewLine]", 
        RowBox[{"aux", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1", ",", 
           RowBox[{"1", "+", 
            RowBox[{"First", "@", "path2pos"}]}]}], "]"}], "]"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"parspos", "=", 
      RowBox[{"Flatten", "@", 
       RowBox[{"Position", "[", 
        RowBox[{"proto", ",", 
         RowBox[{"Except", "[", 
          RowBox[{
          "List", "|", "\"\<path\>\"", "|", "\"\<path1\>\"", "|", 
           "\"\<path2\>\""}], "]"}], ",", "1"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"parsnames", "=", 
      RowBox[{"proto", "[", 
       RowBox[{"[", "parspos", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"parsvalues", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"aux", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1", ",", 
           RowBox[{"1", "+", "#"}]}], "]"}], "]"}], "&"}], "/@", 
       "parspos"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"pars", "=", 
      RowBox[{"AssociationThread", "[", 
       RowBox[{"parsnames", "\[Rule]", "parsvalues"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pars", "=", 
      RowBox[{"PrependTo", "[", 
       RowBox[{"pars", ",", 
        RowBox[{"\"\<head\>\"", "\[Rule]", "head"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"head", ",", "path1", ",", "path2", ",", "pars"}], "}"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
