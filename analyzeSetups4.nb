Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"createGraphFromPrototypes", "::", "usage"}], "=", 
   "\"\<Create the graph of the experimental setup. Input paths are marked \
with green color, output paths with red color. If coincidences are measured, \
corresponding output nodes are marked with black color.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"createGraphFromPrototypes", "[", 
   RowBox[{"prototypes_", ",", 
    RowBox[{"coincidenceList_List:", 
     RowBox[{"{", "}"}]}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "paths", ",", "inputpaths", ",", "outputpaths", ",", "dependencies", ",",
       "lastind", ",", "structure", ",", "lastindicesfree", ",", 
      "lastindicescoinc", ",", "lastindicesassoc", ",", 
      RowBox[{"size", "=", ".2"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"input", " ", "paths"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"paths", "=", 
      RowBox[{"DeleteCases", "[", 
       RowBox[{
        RowBox[{"Union", "[", 
         RowBox[{"Flatten", "@", 
          RowBox[{"prototypes", "[", 
           RowBox[{"[", 
            RowBox[{"All", ",", 
             RowBox[{"{", 
              RowBox[{"2", ",", "3"}], "}"}]}], "]"}], "]"}]}], "]"}], ",", 
        "None"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"inputpaths", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Labeled", "[", 
         RowBox[{
          RowBox[{"Tooltip", "[", 
           RowBox[{"#", ",", 
            RowBox[{"Grid", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"{", 
                RowBox[{"\"\<input\>\"", ",", "#"}], "}"}], "}"}], ",", 
              RowBox[{"Frame", "\[Rule]", "All"}]}], "]"}]}], "]"}], ",", 
          RowBox[{"\"\<Input path \>\"", "<>", 
           RowBox[{"ToString", "[", "#", "]"}]}]}], "]"}], "&"}], "/@", 
       "paths"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"outputpaths", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\"\<Output path \>\"", "<>", 
           RowBox[{"ToString", "[", "#", "]"}]}], ",", "#", ",", "None", ",", 
          RowBox[{"<|", 
           RowBox[{"\"\<output\>\"", "\[Rule]", "#"}], "|>"}]}], "}"}], "&"}],
        "/@", "paths"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dependencies", "=", 
      RowBox[{"AssociationThread", "[", 
       RowBox[{"paths", "\[Rule]", "paths"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"lastind", "=", 
      RowBox[{"Max", "[", "paths", "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"create", " ", "the", " ", "graph", " ", "structure"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"structure", "=", 
      RowBox[{"Fold", "[", 
       RowBox[{"addLayer", ",", 
        RowBox[{"{", 
         RowBox[{"inputpaths", ",", 
          RowBox[{"{", "}"}], ",", "dependencies", ",", "lastind"}], "}"}], 
        ",", "prototypes"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"structure", "=", 
      RowBox[{"Fold", "[", 
       RowBox[{"addLayer", ",", "structure", ",", "outputpaths"}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"output", " ", "paths"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"lastindicesassoc", "=", 
      RowBox[{"structure", "[", 
       RowBox[{"[", 
        RowBox[{"-", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"lastindicescoinc", "=", 
      RowBox[{"DeleteMissing", "@", 
       RowBox[{"Values", "[", 
        RowBox[{"lastindicesassoc", "[", 
         RowBox[{"[", 
          RowBox[{"Key", "/@", "coincidenceList"}], "]"}], "]"}], "]"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"lastindicesfree", "=", 
      RowBox[{"Complement", "[", 
       RowBox[{
        RowBox[{"Values", "[", "lastindicesassoc", "]"}], ",", 
        "lastindicescoinc"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "create", " ", "visual", " ", "representation", " ", "of", " ", "the", 
       " ", "graph"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Graph", "[", 
      RowBox[{
       RowBox[{"Sequence", "@@", 
        RowBox[{"structure", "[", 
         RowBox[{"[", 
          RowBox[{";;", "2"}], "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"GraphLayout", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<LayeredDigraphEmbedding\>\"", ",", " ", 
