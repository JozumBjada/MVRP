Cell[BoxData[{
 RowBox[{"ClearAll", "[", "addLayer", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"addLayer", "::", "usage"}], "=", 
   "\"\<The graph of the experimental setup is built layer by layer. To add a \
new layer, addLayer is used.\n\
addLayer[action_,vertices_,edges_,dependencies_,lastind_]: 'action' is a \
prototype returned by getPrototype, 'vertices' is a list of vertices with \
labels, 'edges' is a list of Rules specifying edges, 'dependencies' is an \
association where keys are input path indices and values are indices of nodes \
where those paths currently end, 'lastind' is the index of the element added \
in the previous run of addLayer.\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{"addLayer", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "vertices_", ",", "edges_", ",", "dependencies_", ",", "lastind_"}], 
     "}"}], ",", "action_"}], "]"}], ":=", 
  RowBox[{"addLayer", "[", 
   RowBox[{
   "action", ",", "vertices", ",", "edges", ",", "dependencies", ",", 
    "lastind"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"addLayer", "[", 
   RowBox[{
   "action_", ",", "vertices_", ",", "edges_", ",", "dependencies_", ",", 
    "lastind_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "edge", ",", "label", ",", "head", ",", "headlabel", ",", "path1", ",", 
      "path2", ",", "pars", ",", 
      RowBox[{"newvert", "=", "vertices"}], ",", 
      RowBox[{"newedges", "=", "edges"}], ",", 
      RowBox[{"newdep", "=", "dependencies"}], ",", 
      RowBox[{"newindex", "=", 
       RowBox[{"lastind", "+", "1"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"new", " ", "action", " ", "to", " ", "add"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"head", ",", "path1", ",", "path2", ",", "pars"}], "}"}], "=", 
      "action"}], ";", "\[IndentingNewLine]", 
     RowBox[{"headlabel", "=", 
      RowBox[{"headLabels", "[", "head", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"head", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"MissingQ", "[", "headlabel", "]"}], ",", "head", ",", 
        "headlabel"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"edge", "=", 
      RowBox[{
       RowBox[{"newdep", "[", "path1", "]"}], "\[Rule]", "newindex"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"newdep", "[", "path1", "]"}], "=", "newindex"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"newedges", ",", "edge"}], "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"path2", "=!=", "None"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"edge", "=", 
         RowBox[{
          RowBox[{"newdep", "[", "path2", "]"}], "\[Rule]", "newindex"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"newdep", "[", "path2", "]"}], "=", "newindex"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"newedges", ",", "edge"}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"label", "=", 
      RowBox[{"Grid", "[", 
       RowBox[{
        RowBox[{"KeyValueMap", "[", 
         RowBox[{"List", ",", "pars"}], "]"}], ",", 
        RowBox[{"Frame", "\[Rule]", "All"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"newvert", ",", 
       RowBox[{"Labeled", "[", 
        RowBox[{
         RowBox[{"Tooltip", "[", 
          RowBox[{"newindex", ",", "label"}], "]"}], ",", "head"}], "]"}]}], 
      "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"newvert", ",", "newedges", ",", "newdep", ",", "newindex"}], 
      "}"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 InitializationCell->True,
